# Google Earth Engine Configuration
# REQUIRED: Your GEE project ID (get from https://code.earthengine.google.com/)
GEE_PROJECT=earthmap-289114

# Optional: Service account credentials for GEE
# GEE_SERVICE_ACCOUNT=your-service-account@your-project.iam.gserviceaccount.com
# GEE_PRIVATE_KEY_PATH=/path/to/private-key.json

# API Configuration
API_TITLE=Whisp API
API_VERSION=1.0.0
HOST=0.0.0.0
PORT=8000
RELOAD=True
LOG_LEVEL=INFO

# Whisp Configuration
WHISP_THRESHOLD_TO_DRIVE=500

# EUDR Risk Thresholds (in percent)
DEFAULT_IND_1_THRESHOLD=10.0
DEFAULT_IND_2_THRESHOLD=10.0
DEFAULT_IND_3_THRESHOLD=0.0
DEFAULT_IND_4_THRESHOLD=0.0

# CORS Settings (adjust for production)
ALLOW_ORIGINS=["*"]
ALLOW_CREDENTIALS=True
ALLOW_METHODS=["*"]
ALLOW_HEADERS=["*"]
